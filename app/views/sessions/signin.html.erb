<% if @user.error.present? %>
    <div style="color: red">
    <h2>Errors prohibited this user from being saved:</h2>

    <ul>
      <li><%= @user.error.full_message %></li>
    </ul>
  </div>
<% end %>
<h1>Sessions#signin</h1>

<%= form_for :session, url: sessions_login_path do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit 'Enter' %>
<% end %>



<%= link_to 'Create a new account', sessions_signup_path %>
